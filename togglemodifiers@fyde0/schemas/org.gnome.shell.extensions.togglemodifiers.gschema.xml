<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema id="org.gnome.shell.extensions.togglemodifiers" path="/org/gnome/shell/extensions/togglemodifiers/">
        <key name="show-ctrl" type="b">
            <default>true</default>
        </key>
        <key name="show-shift" type="b">
            <default>true</default>
        </key>
        <key name="show-alt" type="b">
            <default>true</default>
        </key>
        <key name="padding" type="i">
            <range min="0" max="32"/>
            <default>6</default>
        </key>
    </schema>
</schemalist>